            <script type="text/javascript">
                var $gal = $('#gallery'),
                $sli = $('#slider'),
                $box = $('div', $sli),
                W = $gal.width(), // 500
                N = $box.length,  // 3
                C = 0;            // a counter

                $sli.width((W + 10) * N);

                function slide() {
                    C = (C +1) < 0 ? N - 1 : C % N;
                    $sli.stop().animate({ left: -(C * (W + 10)) }, 800); 
                }

                setInterval(slide, 3000);

            </script>
